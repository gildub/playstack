---
glance::api::database_connection: "mysql://glance:<%= configatron.os.glance.password %>@<%= configatron.ip.internal %>/glance"
glance::api::debug: <%= configatron.os.debug %>
glance::api::host_uri: "http://<%= configatron.ip.public %>:5000"
glance::api::identity_uri: "http://<%= configatron.ip.admin %>:35357/v2.0"
glance::api::keystone_password: <%= configatron.os.glance.password %>
glance::api::registry_host: "<%= configatron.ip.public %>"
glance::api::verbose: <%= configatron.os.verbose %>

glance::backend::file: <%= configatron.os.glance.backend.file %>

glance::db::mysql::password: <%= configatron.os.glance.password %>
glance::db::mysql::host: "<%= configatron.ip.internal %>"

glance::keystone::auth::admin_address: "<%= configatron.ip.admin %>"
glance::keystone::auth::internal_address: "<%= configatron.ip.internal %>"
glance::keystone::auth::password: <%= configatron.os.glance.password %>
glance::keystone::auth::public_address: "<%= configatron.ip.public %>"

glance::registry::debug: <%= configatron.os.debug %>
glance::registry::database_connection: "mysql://glance:<%= configatron.os.glance.password %>@<%= configatron.ip.internal %>/glance"
glance::registry::host_uri: "http://<%= configatron.ip.public %>:5000"
glance::registry::identity_uri: "http://<%= configatron.ip.admin %>:35357/v2.0"
glance::registry::keystone_password: <%= configatron.os.glance.password %>
glance::registry::verbose: <%= configatron.os.verbose %>

#horizon::horizon_ca: horizon_ca
#horizon::horizon_cert: horizon_cert
#horizon::horizon_key: horizon_key
#horizon::neutron_options: { 'enable_lb' => false,  'enable_firewall' => false }
horizon::allowed_hosts: '*'
horizon::compress_offline: false
horizon::django_debug: false
horizon::file_upload_temp_dir: '/var/tmp'
horizon::hypervisor_options:
  can_set_mount_point: false
horizon::keystone_default_role: '_member_'
horizon::keystone_url: "http://<%= configatron.ip.public %>:5000/v2.0"
horizon::listen_ssl: <%= configatron.os.ssl %>
horizon::neutron_options:
  enable_lb: false
  enable_firewall: false
horizon::server_aliases: [ "<%= configatron.ip.public %>", "$::fqdn", "$::hostname", 'localhost' ]
horizon::secret_key: <%= configatron.os.horizon.secret_key %>

# Leave default (0.0.0.0)
#keystone::admin_bind_host: "<%= configatron.ip.admin %>"
keystone::admin_token: <%= configatron.os.admin.token %>
keystone::database_connection: "mysql://keystone:<%= configatron.os.keystone.password %>@<%= configatron.ip.internal %>/keystone"
keystone::debug: <%= configatron.os.debug %>
keystone::enable_ssl: <%= configatron.os.ssl %>
keystone::public_bind_host: "<%= configatron.ip.public %>"
keystone::service_name: <%= configatron.os.keystone.service_name %>
keystone::verbose: <%= configatron.os.verbose %>

keystone::db::mysql::password: <%= configatron.os.keystone.password %>
keystone::db::mysql::host: "<%= configatron.ip.internal %>"
keystone::db::mysql::allowed_hosts: '%'

keystone::endpoint::public_url: "http://<%= configatron.ip.public %>:5000"
keystone::endpoint::admin_url: "http://<%= configatron.ip.admin %>:35357"
keystone::endpoint::internal_url: "http://<%= configatron.ip.internal %>:5000"

keystone::roles::admin::email: <%= configatron.os.admin.email %>
keystone::roles::admin::password: <%= configatron.os.admin.password %>
keystone::roles::admin::admin_tenant: admin

keystone::wsgi::apache::ssl: <%= configatron.os.ssl %>

mysql::server::root_password: <%= configatron.os.dbms.root.password %>
mysql::server::override_options:
  mysqld:
    bind-address: 0.0.0.0
#    bind-address: "<%= configatron.ip.internal %>"
    default_storage_engine: InnoDB

neutron::allow_overlapping_ips: true
neutron::bind_host: "<%= configatron.ip.public %>"
neutron::core_plugin: neutron.plugins.ml2.plugin.Ml2Plugin
neutron::debug: <%= configatron.os.debug %>
neutron::rabbit_host: "<%= configatron.ip.internal %>"
neutron::rabbit_password: <%= configatron.os.rabbitmq.password %>
neutron::rabbit_user: <%= configatron.os.rabbitmq.user %>
neutron::service_plugins:
  - neutron.services.l3_router.l3_router_plugin.L3RouterPlugin
neutron::verbose: <%= configatron.os.verbose %>

neutron::agents::metadata::auth_password: <%= configatron.os.neutron.password %>
neutron::agents::metadata::auth_url: "http://<%= configatron.ip.admin %>:35357/v2.0"
neutron::agents::metadata::metadata_ip: "<%= configatron.ip.public %>"
neutron::agents::metadata::shared_secret: <%= configatron.os.neutron.metadata.shared_secret %>

neutron::agents::ml2::ovs::bridge_uplinks: []
neutron::agents::ml2::ovs::bridge_mappings: []
neutron::agents::ml2::ovs::enable_tunneling: <%= configatron.os.neutron.agents.ml2.ovs.enable_tunneling %>
neutron::agents::ml2::ovs::local_ip: "<%= configatron.os.neutron.agents.ml2.ovs.local_ip %>"
neutron::agents::ml2::ovs::tunnel_types: <%= configatron.os.neutron.agents.ml2.ovs.tunnel_types %>
neutron::agents::ml2::ovs::vxlan_udp_port: <%= configatron.os.neutron.agents.ml2.ovs.vxlan_udp_port %>
neutron::agents::ml2::ovs::l2_population : <%= configatron.os.neutron.agents.ml2.ovs.l2_population %>

neutron::db::mysql::host: "<%= configatron.ip.internal %>"
neutron::db::mysql::password: <%= configatron.os.neutron.password %>

neutron::keystone::auth::admin_address: "<%= configatron.ip.admin %>"
neutron::keystone::auth::internal_address: "<%= configatron.ip.internal %>"
neutron::keystone::auth::password: <%= configatron.os.neutron.password %>
neutron::keystone::auth::public_address: "<%= configatron.ip.public %>"

neutron::plugins::ml2::enable_security_group: <%= configatron.os.neutron.plugins.ml2.enable_security_group %>
neutron::plugins::ml2::flat_networks: <%= configatron.os.neutron.plugins.ml2.flat_networks %>
neutron::plugins::ml2::mechanism_drivers: <%= configatron.os.neutron.plugins.ml2.mechanism_drivers %>
neutron::plugins::ml2::network_vlan_ranges: <%= configatron.os.neutron.plugins.ml2.network_vlan_ranges %>
neutron::plugins::ml2::supported_pci_vendor_devs: <%= configatron.os.neutron.plugins.ml2.supported_pci_vendor_devs %>
neutron::plugins::ml2::sriov_agent_required: <%= configatron.os.neutron.plugins.ml2.sriov_agent_required %>
neutron::plugins::ml2::tenant_network_types: <%= configatron.os.neutron.plugins.ml2.tenant_network_types %>
neutron::plugins::ml2::tunnel_id_ranges: <%= configatron.os.neutron.plugins.ml2.tunnel_id_ranges %>
neutron::plugins::ml2::type_drivers: <%= configatron.os.neutron.plugins.ml2.type_drivers %>
neutron::plugins::ml2::vni_ranges: <%= configatron.os.neutron.plugins.ml2.vni_ranges %>
# neutron::plugins::ml2::vxlan_group:

neutron::server::database_connection: "mysql://neutron:<%= configatron.os.neutron.password %>@<%= configatron.ip.internal %>/neutron"
#neutron::server::auth_host: "<%= configatron.ip.internal %>"
neutron::server::auth_password: <%= configatron.os.neutron.password %>
neutron::server::host_uri: "http://<%= configatron.ip.public %>:5000"
neutron::server::identity_uri: "http://<%= configatron.ip.admin %>:35357/v2.0"
neutron::server::sync_db: true

neutron::server::notifications::nova_admin_auth_url: "http://<%= configatron.ip.admin %>:35357/v2.0"
neutron::server::notifications::nova_admin_password: <%= configatron.os.nova.password %>
neutron::server::notifications::nova_admin_username: nova
neutron::server::notifications::nova_url: "http://<%= configatron.ip.public %>:8774/v2"
neutron::server::notifications::notify_nova_on_port_status_changes: true
neutron::server::notifications::notify_nova_on_port_data_changes: true

nova::database_connection: "mysql://nova:<%= configatron.os.nova.password %>@<%= configatron.ip.internal %>/nova"
nova::debug: <%= configatron.os.debug %>
nova::glance_api_servers: "<%= configatron.ip.public %>:9292"
nova::rabbit_host: "<%= configatron.ip.internal %>"
nova::rabbit_use_ssl: <%= configatron.os.ssl %>
nova::rabbit_userid: <%= configatron.os.rabbitmq.user %>
nova::rabbit_password: <%= configatron.os.rabbitmq.password %>
nova::verbose: <%= configatron.os.verbose %>
#nova::nova_public_key: $public_key
#nova::nova_private_key: $private_key

nova::api::admin_password: <%= configatron.os.nova.password %>
#nova::api::api_bind_address: "<%= configatron.ip.public %>"
nova::api::api_bind_address: 0.0.0.0
nova::api::enabled: true
nova::api::host_uri: "http://<%= configatron.ip.public %>:5000"
nova::api::identity_uri: "http://<%= configatron.ip.admin %>:35357/v2.0"
nova::api::metadata_listen: "<%= configatron.ip.public %>"
nova::api::neutron_metadata_proxy_shared_secret: <%= configatron.os.neutron.metadata.shared_secret %>

nova::compute::enabled: true
nova::compute::libvirt::libvirt_virt_type: qemu

nova::conductor::enabled: true

nova::db::mysql::password: <%= configatron.os.nova.password %>
nova::db::mysql::host: "<%= configatron.ip.internal %>"

nova::keystone::auth::password: <%= configatron.os.nova.password %>
nova::keystone::auth::public_address: "<%= configatron.ip.public %>"
nova::keystone::auth::admin_address: "<%= configatron.ip.admin %>"
nova::keystone::auth::internal_address: "<%= configatron.ip.internal %>"

nova::network::neutron::neutron_admin_password: <%= configatron.os.neutron.password %>
nova::network::neutron::neutron_url: "http://<%= configatron.ip.public %>:9696"
nova::network::neutron::neutron_admin_auth_url: "http://<%= configatron.ip.public %>:35357/v2.0"
#nova::network::neutron::vif_plugging_is_fatal: false
#nova::network::neutron::vif_plugging_timeout: 0

nova::scheduler::enabled: true

rabbitmq::repos_ensure: false
